<?xml version="1.0" encoding="utf-8"?>
<ModMetaData>
	<name>Quest Control</name>
	<author>JoeOwnage67</author>
	<url>https://github.com/JoeOwnage67/</url>
	<supportedVersions>        	
			<li>1.5</li>
    </supportedVersions>
	<packageId>JoeOwnage67.Quest.Control</packageId>  
	
	<loadAfter>
		<li>brrainz.harmony</li>
		<li>sineswiper.xenobionicpatcher</li>
	</loadAfter>

	<loadBefore>
		<li>krkr.rocketman</li>
    </loadBefore>
    <modDependencies>
		<li>
			<packageId>brrainz.harmony</packageId>
			<displayName>Harmony</displayName>
			<steamWorkshopUrl>steam://url/CommunityFilePage/2009463077</steamWorkshopUrl>
			<downloadUrl>https://github.com/pardeike/HarmonyRimWorld/releases/latest</downloadUrl>
		</li>	  
	</modDependencies>
	
	<description>Gives you control which quests can fire during your Rimworld run. Put it at the bottom of your load order before Rocketman, or it will probably miss some quests.</description>	
</ModMetaData>
